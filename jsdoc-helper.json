{
  "description": "generates the documentation for the ES6 module that exports useful help functions for <i>ccmjs</i> component developers.",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./../akless-components/modules/helper.mjs"],
    "includePattern": ".+\\.(js(doc|x)?|mjs)$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "systemName"            : "helper.mjs",
    "footer"                : "<p style='text-align:center'>This Documentation is written by the Developer of <i>ccmjs</i>: <a href='mailto:andre.kless@web.de'>Andr√© Kless</a></p>",
    "copyright"             : "<a href='https://en.wikipedia.org/wiki/MIT_License'>MIT Licence</a>",
    "includeDate"           : true,
    "navType"               : "inline",
    "theme"                 : "cerulean",
    "linenums"              : true,
    "collapseSymbols"       : false,
    "inverseNav"            : true,
    "outputSourceFiles"     : true,
    "outputSourcePath"      : true,
    "dateFormat"            : "",
    "syntaxTheme"           : "default",
    "sort"					: true,
    "search"                : true
  },
  "opts": {
    "destination": "./../akless-components/modules/docs/",
    "encoding": "utf8",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template"
  }
}